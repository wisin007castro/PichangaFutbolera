<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tiger.ghost.pichangafutbolera">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".Inicio">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NuevaCopa" />
        <activity android:name=".NuevoTorneo" />
        <activity android:name=".AgregarEquipoActivity" />
        <activity android:name=".ModificarEquipoActivity" />
        <activity android:name=".ModificarTorneoActivity"></activity>
    </application>

</manifest>